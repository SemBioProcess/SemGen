            <apply>
              <times/>
              <ci> xi </ci> 
              <ci> w </ci>
            </apply>
            <apply>
              <times/>
              <ci> c </ci> 
              <ci> x </ci>
              <apply>
                <plus/>
                <ci> y </ci>
                <ci> I </ci>
              </apply>
              <apply>
                <divide/>
                <ci> z </ci> 
                <apply>
                  <plus/>
                  <apply>
                    <times/>
                    <ci> epsilon </ci> 
                    <ci> x </ci>
                  </apply>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> b </ci> 
            <ci> z </ci>
          </apply> 
        </apply>
      </apply>    
    </math>
