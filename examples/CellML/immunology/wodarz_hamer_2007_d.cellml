            </apply>
            <apply>
              <times/>
              <ci> p </ci> 
              <ci> I </ci>
              <apply>
                <plus/>
                <ci> z </ci>
                <ci> w </ci>
              </apply> 
            </apply>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>

