      <variable name="C_starsum" units="cells_per_GC" public_interface="in"/>
      <variable name="log_total" units="dimensionless"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>total</ci>
            <apply>
               <plus/>
               <ci>B_sum</ci>
               <ci>C_starsum</ci>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>log_total</ci>
            <apply>
               <log/>
               <apply>
                  <plus/>
                  <apply>
                     <divide/>
                     <ci>total</ci>
                     <cn cellml:units="cells_per_GC">1</cn>
                  </apply>
                  <cn type="e-notation" cellml:units="dimensionless">1   <sep/>
