   <component xmlns="http://www.cellml.org/cellml/1.0#" name="alpha_B">
      <variable name="alpha_B" units="dimensionless" public_interface="out"/>
      <variable name="KB" units="dimensionless" initial_value="1e4"/>
      <variable name="B_sum" units="cells_per_GC" public_interface="in"/>
      <variable name="B0" units="cells_per_GC" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>alpha_B</ci>
            <apply>
               <divide/>
               <ci>KB</ci>
               <apply>
                  <plus/>
                  <ci>KB</ci>
                  <apply>
                     <divide/>
                     <ci>B_sum</ci>
                     <cn cellml:units="cells_per_GC">1</cn>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
