      <variable name="KT" units="dimensionless" initial_value="100"/>
      <variable name="T" units="cells_per_GC" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>alpha_T</ci>
            <apply>
               <divide/>
               <ci>KT</ci>
               <apply>
                  <plus/>
                  <ci>KT</ci>
                  <apply>
                     <divide/>
                     <ci>T</ci>
                     <cn cellml:units="cells_per_GC">1</cn>
