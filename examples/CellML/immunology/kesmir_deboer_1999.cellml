   <units xmlns="http://www.cellml.org/cellml/1.0#" name="cells_per_GC" base_units="yes"/>
   <units xmlns="http://www.cellml.org/cellml/1.0#" name="day">
      <unit units="second" multiplier="86400"/>
   </units>
   <units xmlns="http://www.cellml.org/cellml/1.0#" name="first_order_rate_constant">
      <unit units="day" exponent="-1"/>
   </units>
   
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="environment">
      <variable name="time" units="day" public_interface="out"/>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="B0">
      <variable name="B0" units="cells_per_GC" initial_value="3" public_interface="out"/>
      <variable name="pr" units="dimensionless" public_interface="in"/>
      <variable name="mu" units="first_order_rate_constant" public_interface="in"/>
      <variable name="rho" units="first_order_rate_constant" public_interface="in"/>
      <variable name="delta_B" units="first_order_rate_constant" public_interface="in"/>
      <variable name="CT_star" units="cells_per_GC" public_interface="in"/>
      <variable name="time" units="day" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>B0</ci>
            </apply>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>pr</ci>
                  <ci>mu</ci>
                  <ci>CT_star</ci>
               </apply>
               <apply>
                  <plus/>
                  <apply>
                     <times/>
                     <ci>rho</ci>
                     <ci>B0</ci>
                  </apply>
                  <apply>
                     <times/>
                     <ci>delta_B</ci>
                     <ci>B0</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="B1">
      <variable name="B1" units="cells_per_GC" initial_value="0" public_interface="out"/>
      <variable name="alpha_B" units="dimensionless" public_interface="in"/>
