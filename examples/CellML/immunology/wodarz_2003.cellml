                  <times/>
                  <ci>k_2</ci>
                  <apply>
                     <plus/>
                     <ci>I_2</ci>
                     <ci>I_12</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>u</ci>
                  <ci>P_2</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>log_P2</ci>
            <apply>
               <log/>
               <ci>P_2</ci>
            </apply>
         </apply>
      </math>
