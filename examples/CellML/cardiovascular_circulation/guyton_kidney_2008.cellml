		<map_components component_1="normal_urine_volume" component_2="normal_osmolar_and_water_excretion"/>
		<map_variables variable_1="OSMOPN1" variable_2="OSMOPN1"/>
		<map_variables variable_1="OSMOPN" variable_2="OSMOPN"/>
	</connection>
