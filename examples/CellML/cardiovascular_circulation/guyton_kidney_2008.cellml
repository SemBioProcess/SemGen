		<map_variables variable_1="RNAUG4" variable_2="RNAUG4"/>
	</connection>

<!-- PARAMETER CONNECTIONS -->
	<connection>
		<map_components component_1="EAR_calculation" component_2="parameter_values"/>
		<map_variables variable_1="EARK" variable_2="EARK"/>
