   <component name="receptor">
      <variable cmeta:id="receptor_ATP_e" initial_value="0" name="ATP_e" public_interface="out" units="microM"/>
      <variable name="time" public_interface="in" units="second"/>
      <variable name="V_IP3" public_interface="out" units="microMpersec"/>
      <variable initial_value="0" name="MechanicalStimulation" units="dimensionless"/>
      <variable name="V_mech" public_interface="out" units="dimensionless"/>
      <variable initial_value="1" name="V_mech0" units="dimensionless"/>
      <variable initial_value="2.6" name="C_1" public_interface="out" units="microMpersec"/>
      <variable initial_value="0.02" name="C_2" public_interface="out" units="microM"/>
      <variable initial_value="2.3" name="C_3" public_interface="out" units="microMpersec"/>
      <variable initial_value="10" name="C_4" public_interface="out" units="microM"/>
      <variable initial_value="0.001" name="V_ATP" units="microMpersec"/>
      <variable initial_value="0.0001" name="K_ATP" units="microM"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_mech</ci>
            <piecewise>
               <piece>
                  <ci>V_mech0</ci>
                  <apply>
                     <and/>
                     <apply>
                        <gt/>
                        <ci>time</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="second">10</cn>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>time</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="second">25</cn>
                     </apply>
                     <apply>
                        <gt/>
                        <ci>MechanicalStimulation</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0</cn>
                     </apply>
                  </apply>
               </piece>
               <otherwise>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0</cn>
               </otherwise>
            </piecewise>
         </apply>
         <apply>
            <eq/>
            <ci>V_IP3</ci>
            <apply>
               <plus/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <ci>C_1</ci>
                     <ci>ATP_e</ci>
                  </apply>
                  <apply>
                     <plus/>
                     <ci>C_2</ci>
                     <ci>ATP_e</ci>
                  </apply>
               </apply>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <ci>C_3</ci>
                     <apply>
                        <power/>
                        <ci>ATP_e</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
                  <apply>
                     <plus/>
                     <apply>
                        <power/>
                        <ci>C_4</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                     </apply>
                     <apply>
                        <power/>
                        <ci>ATP_e</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>ATP_e</ci>
            </apply>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <apply>
                     <minus/>
                     <ci>V_ATP</ci>
                  </apply>
                  <ci>ATP_e</ci>
               </apply>
               <apply>
                  <plus/>
                  <ci>K_ATP</ci>
                  <ci>ATP_e</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
