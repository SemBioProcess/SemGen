  <units name="micromolar">
    <unit units="mole" prefix="micro"/>
    <unit units="litre" exponent="-1"/>
  </units>
  
  <units name="flux">
    <unit units="micromolar"/>
    <unit units="second" exponent="-1"/>
  </units>
  
  <units name="first_order_rate_constant">
    <unit units="second" exponent="-1"/>
  </units>
  
  <units name="second_order_rate_constant">
    <unit units="micromolar" exponent="-1"/>
    <unit units="second" exponent="-1"/>
  </units>
  
  <units name="micromolar2_per_second">
    <unit units="micromolar" exponent="2"/>
    <unit units="second" exponent="-1"/>
  </units>
  
 
  <component name="environment">
    <variable units="second" public_interface="out" name="time"/>
  </component>
  
  <component name="c" cmeta:id="c">
    <variable units="micromolar" public_interface="out" initial_value="0.01" name="c"/>
    
    <variable units="dimensionless" name="delta" initial_value="0.1"/>
    
    <variable units="flux" public_interface="in" name="J_serca"/>
    <variable units="flux" public_interface="in" name="J_in"/> 
    <variable units="flux" public_interface="in" name="J_pm"/> 
    <variable units="flux" public_interface="in" name="J_IPR"/>
    <variable units="second" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>c</ci> 
        </apply>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <ci> J_IPR </ci>
            <ci> J_serca </ci>
          </apply>
          <apply>
            <times/>
            <ci> delta </ci>
            <apply>
              <minus/>
              <ci> J_in </ci>
              <ci> J_pm </ci>
            </apply>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
  <component name="ce" cmeta:id="ce">
      
    <variable units="micromolar" public_interface="out" initial_value="0.01" name="ce"/>
    
    <variable units="dimensionless" name="gamma" initial_value="5.4"/>
    
    <variable units="flux" public_interface="in" name="J_serca"/>
    <variable units="flux" public_interface="in" name="J_IPR"/>
    <variable units="second" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>ce</ci> 
        </apply>
        <apply>
          <times/>
          <ci> gamma </ci>
          <apply>
            <minus/>
            <ci> J_serca </ci>
            <ci> J_IPR </ci>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
  <component name="p" cmeta:id="p">
      
    <variable units="micromolar" public_interface="out" name="p" initial_value="10.0"/>
  </component>
  
  <component name="J_IPR" cmeta:id="J_IPR">
      
    <variable units="flux" public_interface="out" name="J_IPR"/>
    
    <variable units="first_order_rate_constant" name="kf" initial_value="0.96"/>
    <variable units="first_order_rate_constant" name="g1" initial_value="0.002"/>
    
    <variable units="dimensionless" public_interface="in" name="A"/>
    <variable units="dimensionless" public_interface="in" name="O"/>
    <variable units="micromolar" public_interface="in" name="c"/>
    <variable units="micromolar" public_interface="in" name="ce"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci>J_IPR</ci> 
        <apply>
          <times/>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> kf </ci>
              <apply>
                <power/>
                <apply>
                  <plus/>
                  <apply>
                    <times/>
                    <cn cellml:units="dimensionless"> 0.1 </cn> 
                    <ci> O </ci>
                  </apply>
                  <apply>
                    <times/>
                    <cn cellml:units="dimensionless"> 0.9 </cn> 
                    <ci> A </ci>
                  </apply>
                </apply>
                <cn cellml:units="dimensionless"> 4.0 </cn>
              </apply>
            </apply> 
            <ci> g1 </ci>
          </apply>
          <apply>
            <minus/>
            <ci> ce </ci>
            <ci> c </ci>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
  <component name="R" cmeta:id="R">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.16" name="R"/> 
    
    <variable units="dimensionless" public_interface="in" name="O"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_1"/>
    <variable units="second_order_rate_constant" public_interface="in" name="phi_2"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_2_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_1_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="l_2_"/>
    <variable units="dimensionless" public_interface="in" name="I_1"/> 
    <variable units="micromolar" public_interface="in" name="p"/> 
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>R</ci> 
        </apply>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> phi_2_ </ci>
              <ci> O </ci>
            </apply>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> phi_2 </ci>
                <ci> p </ci>
                <ci> R </ci>
              </apply>
              <apply>
                <times/>
                <ci> phi_1 </ci>
                <ci> R </ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> l_2_  </ci>
              <ci> k_1_  </ci>
            </apply>
            <ci> I_1 </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component>
  
  <component name="O" cmeta:id="O">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.01" name="O"/> 
    
    <variable units="dimensionless" public_interface="in" name="R"/>
    <variable units="dimensionless" public_interface="in" name="A"/>
    <variable units="dimensionless" public_interface="in" name="S"/>
    <variable units="second_order_rate_constant" public_interface="in" name="phi_2"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_2_"/>
    <variable units="second_order_rate_constant" public_interface="in" name="phi_3"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_4"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_4_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_3_"/>
    <variable units="micromolar" public_interface="in" name="p"/> 
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>O</ci> 
        </apply>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> phi_2 </ci>
              <ci> p </ci>
              <ci> R </ci>
            </apply>
            <apply>
              <times/>
              <apply>
                <plus/>
                <ci> phi_2_ </ci>
                <ci> phi_4 </ci>
				<apply>
                  <times/>
				  <cn cellml:units="micromolar"> 1 </cn>
                  <ci> phi_3 </ci>
				</apply>
              </apply>
              <ci> O </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> phi_4_  </ci>
            <ci> A  </ci>
          </apply>
          <apply>
            <times/>
            <ci> k_3_  </ci>
            <ci> S  </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component>
  
  <component name="I_1" cmeta:id="I_1">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.16" name="I_1"/> 
    
    <variable units="dimensionless" public_interface="in" name="R"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_1"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_1_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="l_2_"/>
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>I_1</ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> phi_1 </ci>
            <ci> R </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> k_1_  </ci>
              <ci> l_2_  </ci>
            </apply>
            <ci> I_1  </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component> 
  
  <component name="I_2" cmeta:id="I_2">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.16" name="I_2"/> 
    
    <variable units="dimensionless" public_interface="in" name="A"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_5"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_1_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="l_2_"/>
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>I_2</ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> phi_5 </ci>
            <ci> A </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> k_1_  </ci>
              <ci> l_2_  </ci>
            </apply>
            <ci> I_2  </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component>
  
  <component name="S" cmeta:id="S">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.16" name="S"/> 
    
    <variable units="dimensionless" public_interface="in" name="O"/>
    <variable units="second_order_rate_constant" public_interface="in" name="phi_3"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_3_"/>
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>S</ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <cn cellml:units="micromolar"> 1 </cn>
			<ci> phi_3 </ci>
            <ci> O </ci>
          </apply>
          <apply>
            <times/>
            <ci> k_3_  </ci>
            <ci> S  </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component>
  
  <component name="A" cmeta:id="A">
     
    <variable units="dimensionless" public_interface="out" initial_value="0.16" name="A"/> 
    
    <variable units="dimensionless" public_interface="in" name="O"/>
    <variable units="dimensionless" public_interface="in" name="I_2"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_4"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_4_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="phi_5"/>
    <variable units="first_order_rate_constant" public_interface="in" name="k_1_"/>
    <variable units="first_order_rate_constant" public_interface="in" name="l_2_"/>
    <variable units="second" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>A</ci> 
        </apply>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> phi_4 </ci>
              <ci> O </ci>
            </apply>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> phi_4_ </ci>
                <ci> A </ci>
              </apply>
              <apply>
                <times/>
                <ci> phi_5 </ci>
                <ci> A </ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> k_1_  </ci>
              <ci> l_2_  </ci>
            </apply>
            <ci> I_2  </ci>
          </apply>
        </apply>
      </apply> 
    </math> 
  </component>
  
  <component name="IPR_parameters">
    <variable units="first_order_rate_constant" public_interface="out" name="phi_1"/>
    <variable units="second_order_rate_constant" public_interface="out" name="phi_2"/>
    <variable units="first_order_rate_constant" public_interface="out" name="phi_2_"/>
    <variable units="second_order_rate_constant" public_interface="out" name="phi_3"/>
    <variable units="first_order_rate_constant" public_interface="out" name="phi_4"/>
    <variable units="first_order_rate_constant" public_interface="out" name="phi_4_"/>
    <variable units="first_order_rate_constant" public_interface="out" name="phi_5"/>
    
	<variable units="second_order_rate_constant" name="k_1" initial_value="0.64"/>
    <variable units="first_order_rate_constant" public_interface="out" name="k_1_" initial_value="0.04"/>
    <variable units="second_order_rate_constant" name="k_2" initial_value="37.4"/>
    <variable units="first_order_rate_constant" name="k_2_" initial_value="1.4"/>
    <variable units="second_order_rate_constant" name="k_3" initial_value="0.11"/>
    <variable units="first_order_rate_constant" public_interface="out" name="k_3_" initial_value="29.8"/>
    <variable units="second_order_rate_constant" name="k_4" initial_value="4.0"/> 
    <variable units="first_order_rate_constant" name="k_4_" initial_value="0.54"/>
    <variable units="first_order_rate_constant" name="l_2" initial_value="1.7"/>
    <variable units="first_order_rate_constant"  public_interface="out" name="l_2_" initial_value="0.8"/>  
    <variable units="second_order_rate_constant" name="l_4" initial_value="1.7"/>
    <variable units="second_order_rate_constant" name="l_4_" initial_value="2.5"/>
    <variable units="first_order_rate_constant" name="l_6" initial_value="4707.0"/>
    <variable units="first_order_rate_constant" name="l_6_" initial_value="11.4"/>
    <variable units="micromolar" name="L_1" initial_value="0.12"/>
    <variable units="micromolar" name="L_3" initial_value="0.025"/>
    <variable units="micromolar" name="L_5" initial_value="54.7"/>
    
    <variable units="micromolar" public_interface="in" name="p"/> 
    <variable units="micromolar" public_interface="in" name="c"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="phi_1_calculation">
        <eq/>
        <ci> phi_1 </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> k_1 </ci>
                <ci> L_1 </ci>
              </apply>
              <ci> l_2 </ci>
            </apply>
            <ci> c </ci>
          </apply>
          <apply>
            <plus/>
            <ci> L_1 </ci>
            <apply>
              <times/>
              <ci> c </ci>
              <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <divide/>
                  <ci> L_1 </ci>
                  <ci> L_3 </ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_2_calculation">
        <eq/>
        <ci> phi_2 </ci>
        <apply>
          <divide/>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> k_2 </ci>
              <ci> L_3 </ci>
            </apply>
            <apply>
              <times/>
              <ci> l_4 </ci>
              <ci> c </ci>
            </apply>
          </apply>
          <apply>
            <plus/>
            <ci> L_3 </ci>
            <apply>
              <times/>
              <ci> c </ci>
              <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <divide/>
                  <ci> L_3 </ci>
                  <ci> L_1 </ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_2__calculation">
        <eq/>
        <ci> phi_2_ </ci>
        <apply>
          <divide/>
          <apply>
            <plus/>
            <ci> k_2_ </ci>
            <apply>
              <times/>
              <ci> l_4_ </ci>
              <ci> c </ci>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <divide/>
              <ci> c </ci>
              <ci> L_5 </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_3_calculation">
        <eq/>
        <ci> phi_3 </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> k_3 </ci>
            <ci> L_5 </ci>
          </apply>
          <apply>
            <plus/>
            <ci> c </ci>
            <ci> L_5 </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_4_calculation">
        <eq/>
        <ci> phi_4 </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> k_4 </ci>
                <ci> L_5 </ci>
              </apply>
              <ci> l_6 </ci>
            </apply>
            <ci> c </ci>
          </apply>
          <apply>
            <plus/>
            <ci> c </ci>
            <ci> L_5 </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_4__calculation">
        <eq/>
        <ci> phi_4_ </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> L_1 </ci>
			<apply>
              <plus/>
			  <ci> k_4_ </ci>
              <ci> l_6_ </ci>
            </apply>
		  </apply>
          <apply>
            <plus/>
            <ci> c </ci>
            <ci> L_1 </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="phi_5_calculation">
        <eq/>
        <ci> phi_5 </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <plus/>
			  <apply>
                <times/>
				<ci> k_1 </ci>
				<ci> L_1 </ci>
              </apply>
              <ci> l_2 </ci>
            </apply>
            <ci> c </ci>
          </apply>
          <apply>
            <plus/>
            <ci> c </ci>
            <ci> L_1 </ci>
          </apply>
        </apply>
      </apply>
    </math>     
  </component>    
  
  <component name="J_serca" cmeta:id="J_serca">
      
    <variable units="flux" public_interface="out" name="J_serca"/>
    
    <variable units="micromolar2_per_second" name="Vs" initial_value="120.0"/>
    <variable units="micromolar" name="Ks" initial_value="0.18"/>
    
    <variable units="micromolar" public_interface="in" name="c"/>
    <variable units="micromolar" public_interface="in" name="ce"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci>J_serca</ci> 
        <apply>
          <times/>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> Vs </ci>
              <ci> c </ci>
            </apply>
            <apply>
              <plus/>
              <ci> Ks </ci>
              <ci> c </ci>
            </apply>
          </apply>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <ci> ce </ci> 
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
  <component name="J_pm" cmeta:id="J_pm">
      
    <variable units="flux" public_interface="out" name="J_pm"/>
    
    <variable units="flux" name="Vp" initial_value="28.0"/>
    <variable units="micromolar" name="Kp" initial_value="0.42"/>
    
    <variable units="micromolar" public_interface="in" name="c"/>
    <variable units="micromolar" public_interface="in" name="ce"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci>J_pm</ci> 
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> Vp </ci>
            <apply>
              <power/>
              <ci> c </ci>
              <cn cellml:units="dimensionless"> 2.0 </cn>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <power/>
              <ci> Kp </ci>
              <cn cellml:units="dimensionless"> 2.0 </cn>
            </apply>
            <apply>
              <power/>
              <ci> c </ci>
              <cn cellml:units="dimensionless"> 2.0 </cn>
            </apply>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
  <component name="J_in" cmeta:id="J_in">
      
    <variable units="flux" public_interface="out" name="J_in"/>
    
    <variable units="flux" name="alpha1" initial_value="0.03"/>
    <variable units="first_order_rate_constant" name="alpha2" initial_value="0.2"/>
    
    <variable units="micromolar" public_interface="in" name="p"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci>J_in</ci> 
        <apply>
          <plus/>
          <ci> alpha1 </ci>
          <apply>
            <times/>
            <ci> alpha2 </ci>
            <ci> p </ci>
          </apply>
        </apply>
      </apply>    
    </math>
  </component>
  
