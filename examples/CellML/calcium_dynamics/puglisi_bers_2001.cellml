              <times/>
              <cn cellml:units="dimensionless"> 2.0 </cn>
              <ci> F </ci>
            </apply>
          </apply>
          <apply>
            <ln/>
            <apply>
              <divide/>
              <ci> Cao </ci>
              <ci> Cai </ci>
            </apply>
          </apply>
