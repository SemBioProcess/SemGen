                <apply>
                  <plus/>
                  <cn cellml:units="micromolar"> -7.2 </cn>
                  <ci> p_Ca </ci>
                </apply>
                <cn cellml:units="dimensionless"> 0.6 </cn>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    
