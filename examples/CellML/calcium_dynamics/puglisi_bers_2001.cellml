                <cn cellml:units="millivolt"> 5.0 </cn>
              </apply>
              <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <times/>
                  <cn cellml:units="dimensionless"> 0.051335 </cn>
                  <apply>
                    <exp/>
                    <apply>
                      <divide/>
                      <apply>
                        <plus/>
                        <ci> V </ci>
                        <cn cellml:units="millivolt"> 33.5 </cn>       
                      </apply>
                      <cn cellml:units="millivolt"> 5.0 </cn>
                    </apply>
                  </apply>
                </apply> 
