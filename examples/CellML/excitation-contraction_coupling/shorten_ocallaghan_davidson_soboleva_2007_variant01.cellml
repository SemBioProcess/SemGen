<math xmlns="http://www.w3.org/1998/Math/MathML"> 
<apply id="I_T_Eqn">
        <eq/>

  <ci> I_T </ci>
  
  <apply>
          <times/> 
  
    <apply>
            <divide/> <!-- unit fix 5_08 -->
      <cn cellml:units="millivolt"> 1000 </cn>
      <cn cellml:units="volt"> 1 </cn>
    </apply>
    <apply>
            <divide/>
  
      <apply>
              <minus/>
    
        <ci> vS </ci>
      
        <ci> vT </ci>
      
      </apply>
    
      <ci> R_a </ci>
      
    </apply>
     
  </apply>
 
</apply>
 
</math> 


<math xmlns="http://www.w3.org/1998/Math/MathML"> 
<apply id="vS_diff_calculation">
        <eq/>
  
  <apply>
          <diff/>
    
    <bvar>
            <ci> time </ci>
          </bvar>
 
    <ci> vS </ci>
    
  </apply> 
  
  <apply>
          <minus/> 
    
    <apply>
            <divide/> 
      
      <apply>
              <plus/> 
	
        <ci> I_ionic_s </ci>
	  
        <ci> I_T </ci>
	  
      </apply>
	
      <ci> C_m </ci>
	
    </apply>
      
  </apply>
    
</apply> 
  
</math> 


<math xmlns="http://www.w3.org/1998/Math/MathML"> 
<apply id="dvT_Eqn">
        <eq/>

  <apply>
          <diff/>
    
    <bvar>
            <ci> time </ci>
          </bvar>
 
    <ci> vT </ci>
    
  </apply> 
 
  <apply>
          <minus/>
   
    <apply>
            <divide/>
    
      <apply>
              <minus/>
    
        <ci> I_ionic_t </ci>

        <apply>
                <divide/>
  
          <ci> I_T </ci>
 
          <ci> gam </ci>

        </apply>
  
      </apply>

      <ci> C_m </ci>

    </apply>
  
  </apply>

</apply> 
 
</math> 


