	<ci> P_C_SR </ci>
	<apply>
                <minus/>
	  <ci> PP </ci>
	  <apply>
                  <times/>
	    <ci> P_SR </ci>
	    <apply>
                    <divide/>
	    	<cn cellml:units="M"> 0.001 </cn>
		<cn cellml:units="milliM"> 1 </cn>
	    </apply>
	    <ci> Ca_SR2 </ci>
	  </apply>
	</apply>
	
	<piecewise>
  
          <piece>
           
            <cn cellml:units="dimensionless"> 1 </cn>
            
            <apply>
                    <gt/>
              
              <apply>
                      <minus/>
	        <ci> PP </ci>
	        <apply>
                        <times/>
	          <ci> P_SR </ci>
		    <apply>
                          <divide/>
	          	<cn cellml:units="M"> 0.001 </cn>
			<cn cellml:units="milliM"> 1 </cn>
		    </apply>
	          <ci> Ca_SR2 </ci>
	        </apply>
	      </apply>
              
	      <cn cellml:units="milliM2"> 0 </cn>
            
            </apply>
          
          </piece>
      
          <otherwise>
            
            <cn cellml:units="dimensionless"> 0 </cn>
                
          </otherwise>
        
        </piecewise>
	
	
	
      </apply>
    </apply>
    
    </apply>
</apply>
</math>
    
  
        
