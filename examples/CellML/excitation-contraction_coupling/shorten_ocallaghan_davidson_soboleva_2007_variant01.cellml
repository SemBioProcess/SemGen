  <ci> I_ionic_s </ci> 
  
  <apply>
          <plus/> 
    
    <ci> I_Cl </ci>
 
    <ci> I_IR </ci>
  
    <ci> I_DR </ci>

    <ci> I_Na </ci>

    <ci> I_NaK </ci>
    
    <apply>
            <minus/>
    
    	<ci> I_HH </ci>
	
    </apply>
    
  </apply>
    
</apply> 
 
</math> 
 
 

