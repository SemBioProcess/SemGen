              <plus/>
              <apply>
                <power/>
		        <ci> K_A </ci>
                <ci> p </ci>
			  </apply>
		      <apply>
                <power/>
		        <ci> Ca_in </ci>
                <ci> p </ci>
			  </apply>
	        </apply>
          </apply>
        </apply>
      </apply>
