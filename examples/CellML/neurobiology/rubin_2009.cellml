   </component>
   
   <component name="V1">
      <variable units="millivolt" public_interface="out" name="V1" initial_value="-50.0"/>
      
	  <variable units="nanoF" public_interface="in" name="C"/>
	  <variable units="nanoA" public_interface="in" name="i_NaP"/>
      <variable units="nanoA" public_interface="in" name="i_K"/>
      <variable units="nanoA" public_interface="in" name="i_L1"/>
      <variable units="nanoA" public_interface="in" name="i_synE1"/>
      <variable units="nanoA" public_interface="in" name="i_synI1"/>
      <variable units="millisecond" public_interface="in" name="time"/>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>V1</ci>
            </apply>
            <apply>
               <divide/>
               <apply>
                  <minus/>
                  <apply>
                     <plus/>
                     <ci>i_NaP</ci>
                     <ci>i_K</ci>
                     <ci>i_L1</ci>
                     <ci>i_synE1</ci>
				 	 <ci>i_synI1</ci>
                  </apply>
               </apply>
               <ci>C</ci>
            </apply>
         </apply>
      </math>
   </component>
   
