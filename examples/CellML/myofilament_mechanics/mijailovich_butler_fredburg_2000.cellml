    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>M</ci> 
        </apply>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> M </ci>
			</apply>
