  <units name="mm">
     <unit units="meter" prefix="milli"/>
  </units>
  <units name="mm_per_second">
     <unit units="mm"/>
     <unit units="second" exponent="-1"/>
  </units>
  <units name="per_mm">
     <unit units="mm" exponent="-1"/>
  </units>
  <units name="newton_per_mm">
     <unit units="newton"/>
     <unit units="mm" exponent="-1"/>
  </units>
  <units name="mm_newton_per_second">
     <unit units="mm"/>
	 <unit units="newton"/>
     <unit units="second" exponent="-1"/>
  </units>
  
  <component name="environment">
     <variable name="time" units="second" public_interface="out"/>
  </component>
  
  <component name="contraction">
     <variable initial_value="-24" name="L_1" units="mm"/>
     <variable initial_value="-14" name="L_2" units="mm"/>
     <variable initial_value="4" name="L_3" units="mm"/>
     <variable initial_value="14" name="L_4" units="mm"/>
     <variable initial_value="0.51" name="f_c" units="newton"/>
     <variable initial_value="-152" name="v_max" units="mm_per_second"/>
     <variable initial_value="9.5" name="curv" units="dimensionless"/>
     <variable initial_value="0.0064" name="k_1" units="newton"/>
     <variable initial_value="0.414" name="k_2" units="per_mm"/>
     <variable initial_value="8.9" name="F_1" units="newton"/>
     <variable initial_value="2.9" name="d_LSEC1" units="mm"/>
     <variable initial_value="2.5" name="k_sh" units="dimensionless"/>
     <variable initial_value="2.6" name="L_m" units="mm"/>
     <variable initial_value="20.6" name="F_im" units="newton"/>
     <variable initial_value="0.065" name="tau" units="second"/>
     <variable name="v_cc" units="mm_per_second"/>
     <variable name="f_v" units="dimensionless"/>
     <variable name="f_L" units="newton"/>
     <variable name="f_sec" units="newton"/>
     <variable name="time" public_interface="in" units="second"/>
     <variable name="delta_L_sec" units="mm"/>
     <variable initial_value="4.1" name="delta_L_sec1" units="mm"/>
     <variable initial_value="8.2" name="k" units="newton_per_mm"/>
     <variable initial_value="0.2" name="L_cc" units="mm"/>
     <variable name="L_mtc" units="mm"/>
     <variable initial_value="1" name="A" units="dimensionless"/>
     
	 <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
		  <eq/>
		  <ci>delta_L_sec</ci>
		  <apply>
		    <minus/>
			<apply>
			  <minus/>
			  <ci>L_mtc</ci>
			  <ci>L_cc</ci>
			</apply>
			<ci>L_m</ci>
		  </apply>
		</apply>
        
		<apply>
           <eq/>
           <ci>f_sec</ci>
           <piecewise>
              <piece>
                 <apply>
                    <times/>
                    <apply>
                       <divide/>
                       <ci>F_1</ci>
                       <apply>
                          <minus/>
                          <apply>
                             <exp/>
                             <ci>k_sh</ci>
                          </apply>
                          <cn cellml:units="dimensionless">1</cn>
                       </apply>
                    </apply>
                    <apply>
                       <minus/>
                       <apply>
                          <exp/>
                          <apply>
                             <divide/>
                             <apply>
                                <times/>
                                <ci>k_sh</ci>
                                <ci>delta_L_sec</ci>
                             </apply>
                             <ci>delta_L_sec1</ci>
                          </apply>
                       </apply>
                       <cn cellml:units="dimensionless">1</cn>
                    </apply>
                 </apply>
                 <apply>
                    <and/>
                    <apply>
                       <gt/>
                       <ci>delta_L_sec</ci>
                       <cn cellml:units="mm">0</cn>
                    </apply>
                    <apply>
                       <lt/>
                       <ci>delta_L_sec</ci>
                       <ci>delta_L_sec1</ci>
                    </apply>
                 </apply>
              </piece>
              <piece>
                 <apply>
                    <plus/>
                    <ci>F_1</ci>
                    <apply>
                       <times/>
                       <ci>k</ci>
                       <apply>
                          <minus/>
                          <ci>delta_L_sec</ci>
                          <ci>delta_L_sec1</ci>
                       </apply>
                    </apply>
                 </apply>
                 <apply>
                    <leq/>
                    <ci>delta_L_sec</ci>
                    <ci>delta_L_sec1</ci>
                 </apply>
              </piece>
           </piecewise>
        </apply>
        
		<apply>
          <eq/>
          <ci>v_cc</ci>
          <apply>
            <times/>
            <apply>
              <divide/>
              <cn cellml:units="mm_newton_per_second">1</cn>
              <ci>f_v</ci>
            </apply>
            <apply>
              <divide/>
              <ci>f_sec</ci>
              <apply>
                <times/>
                <ci>A</ci>
                <ci>f_L</ci>
                <ci>F_im</ci>
              </apply>
            </apply>
          </apply>
        </apply>

        <apply>
           <eq/>
           <apply>
              <diff/>
              <bvar>
                 <ci>time</ci>
              </bvar>
              <ci>L_cc</ci>
           </apply>
           <ci>v_cc</ci>
        </apply>
        
		<apply>
           <eq/>
           <ci>f_v</ci>
           <apply>
              <divide/>
              <apply>
                 <minus/>
                 <ci>v_max</ci>
                 <ci>v_cc</ci>
              </apply>
              <apply>
                 <plus/>
                 <ci>v_max</ci>
                 <apply>
                    <times/>
                    <ci>v_cc</ci>
                    <ci>curv</ci>
                 </apply>
              </apply>
           </apply>
        </apply>
        
		<apply>
           <eq/>
           <ci>f_L</ci>
           <piecewise>
              <piece>
                 <apply>
                    <times/>
                    <apply>
                       <divide/>
                       <ci>f_c</ci>
                       <apply>
                          <minus/>
                          <ci>L_2</ci>
                          <ci>L_1</ci>
                       </apply>
                    </apply>
                    <apply>
                       <minus/>
                       <ci>L_cc</ci>
                       <ci>L_1</ci>
                    </apply>
                 </apply>
                 <apply>
                    <and/>
                    <apply>
                       <geq/>
                       <ci>L_cc</ci>
                       <ci>L_1</ci>
                    </apply>
                    <apply>
                       <leq/>
                       <ci>L_cc</ci>
                       <ci>L_2</ci>
                    </apply>
                 </apply>
              </piece>
              <piece>
                 <apply>
                    <times/>
                    <apply>
                       <divide/>
                       <apply>
                          <minus/>
                          <cn cellml:units="newton">1</cn>
                          <ci>f_c</ci>
                       </apply>
                       <apply>
                          <minus/>
                          <ci>L_2</ci>
                       </apply>
                    </apply>
                    <apply>
                       <minus/>
                       <ci>L_cc</ci>
                       <ci>L_2</ci>
                    </apply>
                 </apply>
                 <apply>
                    <and/>
                    <apply>
                       <gt/>
                       <ci>L_cc</ci>
                       <ci>L_2</ci>
                    </apply>
                    <apply>
                       <leq/>
                       <ci>L_cc</ci>
                       <cn cellml:units="mm">0</cn>
                    </apply>
                 </apply>
              </piece>
              <piece>
                 <cn cellml:units="newton">1</cn>
                 <apply>
                    <and/>
                    <apply>
                       <gt/>
                       <ci>L_cc</ci>
                       <cn cellml:units="mm">0</cn>
                    </apply>
                    <apply>
                       <leq/>
                       <ci>L_cc</ci>
                       <ci>L_3</ci>
                    </apply>
                 </apply>
              </piece>
              <piece>
                 <apply>
                    <times/>
                    <apply>
                       <divide/>
                       <apply>
                          <minus/>
                          <cn cellml:units="newton">1</cn>
                       </apply>
                       <apply>
                          <minus/>
                          <ci>L_4</ci>
                          <ci>L_3</ci>
                       </apply>
                    </apply>
                    <apply>
                       <minus/>
                       <ci>L_cc</ci>
                       <ci>L_3</ci>
                    </apply>
                 </apply>
                 <apply>
                    <and/>
                    <apply>
                       <gt/>
                       <ci>L_cc</ci>
                       <ci>L_3</ci>
                    </apply>
                    <apply>
                       <leq/>
                       <ci>L_cc</ci>
                       <ci>L_4</ci>
                    </apply>
                 </apply>
              </piece>
           </piecewise>
        </apply>
        
		<apply>
           <eq/>
           <ci>L_mtc</ci>
           <piecewise>
              <piece>
                 <cn cellml:units="mm">0.29</cn>
                 <apply>
                    <leq/>
                    <ci>time</ci>
                    <cn cellml:units="second">1</cn>
                 </apply>
              </piece>
              <piece>
                 <cn cellml:units="mm">0.22</cn>
                 <apply>
                    <and/>
                    <apply>
					<gt/>
                       <ci>time</ci>
                       <cn cellml:units="second">1</cn>
                    </apply>
                    <apply>
                       <lt/>
                       <ci>time</ci>
                       <cn cellml:units="second">5</cn>
                    </apply>
                 </apply>
              </piece>
              <otherwise>
                 <cn cellml:units="mm">0.19</cn>
              </otherwise>
           </piecewise>
        </apply>
     </math>
  </component>
  
  <connection>
     <map_components component_1="environment" component_2="contraction"/>
     <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
