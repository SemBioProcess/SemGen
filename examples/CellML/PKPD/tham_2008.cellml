      <variable initial_value="604800" name="conversion_factor" units="sec_per_week"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>Exposure</ci>
            <piecewise>
               <piece>
                  <ci>Dose</ci>
                  <apply>
                     <and/>
