                     </apply>
                     <apply>
                        <lt/>
                        <ci>rem_time</ci>
                        <ci>Dose_Length</ci>
                     </apply>
                  </apply>
               </piece>
               <piece>
                  <ci>Dose</ci>
                  <apply>
                     <and/>
                     <apply>
                        <lt/>
                        <ci>time</ci>
                        <apply>
                           <times/>
                           <ci>Cycle_Int</ci>
                           <ci>N_Cycle</ci>
                        </apply>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>Dose_Int2</ci>
                        <ci>rem_time</ci>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>rem_time</ci>
                        <apply>
                           <plus/>
