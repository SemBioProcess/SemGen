        </apply>
        <apply>
          <plus/> 
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k1 </ci>
              <apply>
                <plus/>
                <ci> r </ci>
                <ci> c </ci>
              </apply>
              <ci> f </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> k2 </ci>
              <apply>
                <times/>
