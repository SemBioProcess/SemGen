        </apply>  
      </apply> 
    </math> 
  </component>

  <component cmeta:id="CHOC" name="CHOC">
    <variable units="micromolar" public_interface="out" cmeta:id="CHOC_CHOC" name="CHOC" initial_value="532.0"/> 
    
    <variable units="first_order_rate_constant" public_interface="in" name="Lmtr"/>
    <variable units="first_order_rate_constant" public_interface="in" name="Kmtr"/>
    <variable units="micromolar" public_interface="in" name="CHOM"/>
