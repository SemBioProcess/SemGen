  <component cmeta:id="cAMP" name="cAMP">
    <variable units="micromolar" public_interface="out" cmeta:id="cAMP_cAMP" name="cAMP" initial_value="0.95"/> 
    
    <variable units="dimensionless" name="Ko" initial_value="0.013"/>
    <variable units="per_micromolar" name="Ka" initial_value="10"/>
    <variable units="per_micromolar" name="Kb" initial_value="1000000.0"/>
    <variable units="micromolar" name="Kdsm" initial_value="10.0"/>
    <variable units="flux" name="Vmsm" initial_value="6.0"/>
    <variable units="flux" name="Vdsm" initial_value="10.0"/>
    <variable units="micromolar" cmeta:id="cAMP_ACTH" name="ACTH"/>
    
    <variable units="minute" public_interface="in" name="time"/> 
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> cAMP </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <divide/> 
            <apply>
              <times/>
              <ci> Vmsm </ci>
              <ci> Ko </ci>
              <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <times/>
                  <ci> Kb </ci>
                  <ci> ACTH </ci>
                </apply>
              </apply>
            </apply>
            <apply>
              <plus/>
	          <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <times/>
                  <ci> Ka </ci>
                  <ci> ACTH </ci>
		        </apply>
              </apply>
              <apply>
                <times/>
                <ci> Ko </ci>
                <apply>
                  <plus/>
                  <cn cellml:units="dimensionless"> 1.0 </cn>
                  <apply>
                    <times/>
                    <ci> Kb </ci>
                    <ci> ACTH </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <divide/> 
            <apply>
              <times/>
              <ci> Vdsm </ci>
              <ci> cAMP </ci>
            </apply>
            <apply>
              <plus/>
              <ci> Kdsm </ci>
              <ci> cAMP </ci>
            </apply>
          </apply>
        </apply>  
      </apply>

      <apply>
        <eq/>
        <ci> ACTH </ci>
        <piecewise>
          <piece>
            <cn type="e-notation" cellml:units="micromolar">12<sep/>-6</cn>
			<apply>
              <and/>
              <apply>
                <geq/>
                <ci> time </ci>
                <cn cellml:units="minute"> 0.0 </cn>
              </apply>
              <apply>
                <lt/>
                <ci> time </ci>
                <cn cellml:units="minute"> 10.0 </cn>
              </apply>
            </apply>
          </piece>
          <piece>
            <cn type="e-notation" cellml:units="micromolar">16<sep/>-6</cn>
            <apply>
              <and/>
              <apply>
                <geq/>
                <ci> time </ci>
                <cn cellml:units="minute"> 10.0 </cn>
              </apply>
              <apply>
                <lt/>
                <ci> time </ci>
                <cn cellml:units="minute"> 20.0 </cn>
              </apply>
            </apply>
          </piece>
          <piece>
            <cn type="e-notation" cellml:units="micromolar">12<sep/>-6</cn>
            <apply>
              <and/>
              <apply>
                <geq/>
                <ci> time </ci>
                <cn cellml:units="minute"> 20.0 </cn>
              </apply>
              <apply>
                <lt/>
                <ci> time </ci>
                <cn cellml:units="minute"> 35.0 </cn>
              </apply>
            </apply>
          </piece>
          <piece>
            <cn type="e-notation" cellml:units="micromolar">16<sep/>-6</cn>
            <apply>
              <and/>
              <apply>
                <geq/>
                <ci> time </ci>
                <cn cellml:units="minute"> 35.0 </cn>
              </apply>
              <apply>
                <lt/>
                <ci> time </ci>
                <cn cellml:units="minute"> 45.0 </cn>
              </apply>
            </apply>
          </piece>
          <otherwise>
             <cn type="e-notation" cellml:units="micromolar">12<sep/>-6</cn>
          </otherwise>
        </piecewise>
      </apply>
    </math> 
  </component>

  <component name="IS">
    <variable units="dimensionless" public_interface="out" cmeta:id="IS_IS" name="IS"/> 
    
    <variable units="dimensionless" name="Crpt" initial_value="3.0"/>
    <variable units="dimensionless" name="K" initial_value="80.0"/>
    <variable units="micromolar" name="Kd" initial_value="2.11"/>
    <variable units="dimensionless" name="n" initial_value="4.0"/>
    
    <variable units="micromolar" public_interface="in" name="cAMP"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci> IS </ci> 
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> K </ci>
            <ci> Crpt </ci>
            <apply>
              <power/>
              <ci> cAMP </ci>
              <ci> n </ci>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <power/>
              <ci> Kd </ci>
              <ci> n </ci>
            </apply>
            <apply>
              <power/>
              <ci> cAMP </ci>
              <ci> n </ci>
            </apply>
          </apply>
        </apply>  
      </apply> 
    </math> 
  </component>
  
  <component cmeta:id="V" name="V">
