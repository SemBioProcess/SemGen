      <variable name="time" public_interface="in" units="millisecond"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>V</ci>
            </apply>
            <apply>
               <divide/>
               <apply>
                  <minus/>
                  <apply>
                     <plus/>
                     <ci>I_Ca</ci>
                     <ci>I_K</ci>
                     <ci>I_SK</ci>
                     <ci>I_DA</ci>
                  </apply>
               </apply>
               <ci>C</ci>
            </apply>
         </apply>
      </math>
