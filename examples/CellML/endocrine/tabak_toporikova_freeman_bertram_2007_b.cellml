         <apply>
            <eq/>
            <ci>I_K</ci>
            <apply>
               <times/>
               <ci>gK</ci>
               <ci>n</ci>
               <apply>
                  <minus/>
                  <ci>V</ci>
                  <ci>VK</ci>
               </apply>
            </apply>
         </apply>
      </math>
