      <variable units="dimensionless" public_interface="in" name="Wee1P"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>kcdc25</ci>
            <apply>
               <plus/>
               <apply>
                  <times/>
                  <ci>V25</ci>
                  <ci>Cdc25</ci>
               </apply>
               <apply>
                  <times/>
                  <ci>V25_</ci>
                  <ci>Cdc25P</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>k2</ci>
            <apply>
               <plus/>
               <apply>
                  <times/>
                  <ci>V2</ci>
                  <ci>APC</ci>
               </apply>
               <apply>
                  <times/>
                  <ci>V2_</ci>
                  <ci>APC_</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>kwee</ci>
            <apply>
               <plus/>
               <apply>
                  <times/>
                  <ci>Vwee</ci>
                  <ci>Wee1P</ci>
               </apply>
               <apply>
                  <times/>
                  <ci>Vwee_</ci>
                  <ci>Wee1</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
