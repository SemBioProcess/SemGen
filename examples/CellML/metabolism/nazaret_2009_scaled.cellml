		  <apply>
		    <divide/>
			<cn cellml:units="dimensionless"> 1 </cn>
			<apply>
		      <plus/>
			  <cn cellml:units="dimensionless"> 1 </cn>
			  <apply>
		        <exp/>
		        <apply>
		          <times/>
			      <ci> delta_r2 </ci>
				  <apply>
		            <minus/>
				    <ci> s </ci>
			        <cn cellml:units="dimensionless"> 1 </cn>
				  </apply>
		        </apply>
			  </apply>
			</apply>
		  </apply>
		</apply>
