      <apply id="vATP_calculation"><eq/>
        <ci> vATP </ci>
        <apply>
		  <times/>
          <ci> beta_ATP </ci>
		  <apply>
		    <minus/>
			<apply>
		      <divide/>
			  <cn cellml:units="dimensionless"> 2 </cn>
			  <apply>
		        <plus/>
			    <cn cellml:units="dimensionless"> 1 </cn>
			    <apply>
		          <exp/>
		          <apply>
		            <times/>
			        <ci> delta_atp </ci>
				    <apply>
		              <minus/>
				      <ci> en </ci>
			          <apply>
		                <times/>
						<ci> en_crit </ci>
						<ci> s </ci>
					  </apply>
					</apply>
				  </apply>
				</apply>
			  </apply>
			</apply>
			<cn cellml:units="dimensionless"> 1 </cn>
		  </apply>
		</apply>
      </apply>
    </math>
  </component>
  
