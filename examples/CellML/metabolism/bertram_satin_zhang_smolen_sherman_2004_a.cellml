      <component name="environment">
            <variable cmeta:id="environment_time" name="time" public_interface="out" units="millisecond"/>
      </component>
      <component name="membrane">
            <variable cmeta:id="membrane_V" initial_value="-60" name="V" public_interface="out" units="millivolt"/>
            <variable cmeta:id="membrane_IK" name="IK" units="femtoA"/>
            <variable name="ICa" public_interface="out" units="femtoA"/>
            <variable cmeta:id="membrane_IKCa" name="IKCa" units="femtoA"/>
            <variable name="IKATP" public_interface="in" units="femtoA"/>
            <variable initial_value="5300" name="Cm" units="femtoF"/>
            <variable initial_value="2700" name="gK" units="picoS"/>
            <variable name="VK" public_interface="in" units="millivolt"/>
            <variable initial_value="0" name="n" units="dimensionless"/>
            <variable initial_value="100" name="gKCa" units="picoS"/>
            <variable initial_value="0.5" name="kd" units="micromolar"/>
            <variable name="c" public_interface="in" units="micromolar"/>
            <variable initial_value="1000" name="gCa" units="picoS"/>
            <variable name="minf" units="dimensionless"/>
            <variable initial_value="25" name="VCa" units="millivolt"/>
            <variable initial_value="20" name="taun" units="millisecond"/>
            <variable name="time" public_interface="in" units="millisecond"/>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t membrane voltage (V)">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>V</ci>
                        </apply>
                        <apply>
                              <divide/>
                              <apply>
                                    <minus/>
                                    <apply>
                                          <plus/>
                                          <ci>IK</ci>
                                          <ci>ICa</ci>
                                          <ci>IKCa</ci>
                                          <ci>IKATP</ci>
                                    </apply>
                              </apply>
                              <ci>Cm</ci>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="IK">
                  <apply>
                        <eq/>
                        <ci>IK</ci>
                        <apply>
                              <times/>
                              <ci>gK</ci>
                              <ci>n</ci>
                              <apply>
                                    <minus/>
                                    <ci>V</ci>
                                    <ci>VK</ci>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="IKCa">
                  <apply>
                        <eq/>
                        <ci>IKCa</ci>
                        <apply>
                              <times/>
                              <apply>
                                    <divide/>
                                    <ci>gKCa</ci>
                                    <apply>
                                          <plus/>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                                          <apply>
                                                <power/>
                                                <apply>
                                                      <divide/>
                                                      <ci>kd</ci>
                                                      <ci>c</ci>
                                                </apply>
                                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                                          </apply>
                                    </apply>
                              </apply>
                              <apply>
                                    <minus/>
                                    <ci>V</ci>
                                    <ci>VK</ci>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="ICa">
                  <apply>
                        <eq/>
                        <ci>ICa</ci>
                        <apply>
                              <times/>
                              <ci>gCa</ci>
                              <ci>minf</ci>
                              <apply>
                                    <minus/>
                                    <ci>V</ci>
                                    <ci>VCa</ci>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="minf">
                  <apply>
                        <eq/>
                        <ci>minf</ci>
                        <apply>
                              <divide/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                              <apply>
                                    <plus/>
                                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                                    <apply>
                                          <exp/>
                                          <apply>
                                                <divide/>
                                                <apply>
                                                      <minus/>
                                                      <apply>
                                                            <plus/>
                                                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millivolt">20</cn>
                                                            <ci>V</ci>
                                                      </apply>
                                                </apply>
                                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millivolt">12</cn>
                                          </apply>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="ninf">
                  <apply>
                        <eq/>
                        <ci>ninf</ci>
                        <apply>
                              <divide/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                              <apply>
                                    <plus/>
                                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                                    <apply>
                                          <exp/>
                                          <apply>
                                                <divide/>
                                                <apply>
                                                      <minus/>
                                                      <apply>
                                                            <plus/>
                                                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millivolt">16</cn>
                                                            <ci>V</ci>
                                                      </apply>
                                                </apply>
                                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millivolt">5</cn>
                                          </apply>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t delayed K+ rectifier activation (n)">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>n</ci>
                        </apply>
                        <apply>
                              <divide/>
                              <apply>
                                    <minus/>
                                    <ci>ninf</ci>
                                    <ci>n</ci>
                              </apply>
                              <ci>taun</ci>
                        </apply>
                  </apply>
            </math>
            <variable name="ninf" units="dimensionless"/>
      </component>
      <component name="calcium_handling">
            <variable name="time" public_interface="in" units="millisecond"/>
            <variable initial_value="0.1" name="c" public_interface="out" units="micromolar"/>
            <variable initial_value="185" name="cer" public_interface="out" units="micromolar"/>
            <variable initial_value="0.01" name="fcyt" units="dimensionless"/>
            <variable name="Jmem" units="flux"/>
            <variable name="Jer" units="flux"/>
            <variable initial_value="0.01" name="fer" units="dimensionless"/>
            <variable initial_value="31" name="sigmaV" units="dimensionless"/>
            <variable initial_value="0.0002" name="pleak" units="first_order_rate_constant"/>
            <variable initial_value="0.4" name="Kserca" units="first_order_rate_constant"/>
            <variable initial_value="1" name="lambdaer" units="dimensionless"/>
            <variable initial_value="1" name="epser" units="dimensionless"/>
            <variable initial_value="0.00000450" name="alpha" units="micromolar_per_femtoA_millisecond"/>
            <variable name="ICa" public_interface="in" units="femtoA"/>
            <variable initial_value="0.2" name="kpmca" units="first_order_rate_constant"/>
            <variable cmeta:id="calcium_handling_Jserca" name="Jserca" units="flux"/>
            <variable cmeta:id="calcium_handling_Jleak" name="Jleak" units="flux"/>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t free cytosolic Ca conc">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>c</ci>
                        </apply>
                        <apply>
                              <times/>
                              <ci>fcyt</ci>
                              <apply>
                                    <plus/>
                                    <ci>Jmem</ci>
                                    <ci>Jer</ci>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t free ER Ca conc">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>cer</ci>
                        </apply>
                        <apply>
                              <times/>
                              <apply>
                                    <minus/>
                                    <ci>fer</ci>
                              </apply>
                              <ci>sigmaV</ci>
                              <ci>Jer</ci>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="Jserca">
                  <apply>
                        <eq/>
                        <ci>Jserca</ci>
                        <apply>
                              <times/>
                              <ci>Kserca</ci>
                              <ci>c</ci>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="Jleak">
                  <apply>
                        <eq/>
                        <ci>Jleak</ci>
                        <apply>
                              <times/>
                              <ci>pleak</ci>
                              <apply>
                                    <minus/>
                                    <ci>cer</ci>
                                    <ci>c</ci>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="Jer">
                  <apply>
                        <eq/>
                        <ci>Jer</ci>
                        <apply>
                              <divide/>
                              <apply>
                                    <times/>
                                    <ci>epser</ci>
                                    <apply>
                                          <minus/>
                                          <ci>Jleak</ci>
                                          <ci>Jserca</ci>
                                    </apply>
                              </apply>
                              <ci>lambdaer</ci>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="Jmem">
                  <apply>
                        <eq/>
                        <ci>Jmem</ci>
                        <apply>
                              <minus/>
                              <apply>
                                    <plus/>
                                    <apply>
                                          <times/>
                                          <ci>alpha</ci>
                                          <ci>ICa</ci>
                                    </apply>
                                    <apply>
                                          <times/>
                                          <ci>kpmca</ci>
                                          <ci>c</ci>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
      </component>
      <component name="glycolysis">
            <variable name="time" public_interface="in" units="millisecond"/>
            <variable name="rgpdh" public_interface="out" units="flux"/>
            <variable initial_value="0.2" name="Rgk" units="per_second"/>
            <variable initial_value="3000" name="atot" public_interface="out" units="micromolar"/>
            <variable initial_value="0.06" name="pfkbas" public_interface="out" units="dimensionless"/>
            <variable name="f6p" public_interface="out" units="micromolar"/>
            <variable initial_value="0.005" name="lambda" units="dimensionless"/>
            <variable name="pfk" public_interface="in" units="micromolar"/>
            <variable initial_value="200" name="g6p" units="micromolar"/>
            <variable initial_value="40" name="fbp" public_interface="out" units="micromolar"/>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="f6p">
                  <apply>
                        <eq/>
                        <ci>f6p</ci>
                        <apply>
                              <times/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0.3</cn>
                              <ci>g6p</ci>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="rgpdh">
                  <apply>
                        <eq/>
                        <ci>rgpdh</ci>
                        <apply>
                              <times/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="flux">0.2</cn>
                              <apply>
                                    <root/>
                                    <apply>
                                          <divide/>
                                          <ci>fbp</ci>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar">1</cn>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t fbp">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>fbp</ci>
                        </apply>
                        <apply>
                              <times/>
                              <ci>lambda</ci>
                              <apply>
                                    <minus/>
                                    <apply>
                                          <divide/>
                                          <ci>pfk</ci>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millisecond">1</cn>
                                    </apply>
                                    <apply>
                                          <times/>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0.5</cn>
                                          <ci>rgpdh</ci>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" id="delta/t g6p">
                  <apply>
                        <eq/>
                        <apply>
                              <diff/>
                              <bvar>
                                    <ci>time</ci>
                              </bvar>
                              <ci>g6p</ci>
                        </apply>
                        <apply>
                              <times/>
                              <ci>lambda</ci>
                              <apply>
                                    <minus/>
                                    <apply>
                                          <times/>
                                          <ci>Rgk</ci>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar">1</cn>
                                    </apply>
                                    <apply>
                                          <divide/>
                                          <ci>pfk</ci>
                                          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="second">1</cn>
                                    </apply>
                              </apply>
                        </apply>
                  </apply>
            </math>
      </component>
