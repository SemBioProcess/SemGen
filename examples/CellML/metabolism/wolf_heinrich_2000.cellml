      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>S4</ci>
        </apply>
        <apply>
          <minus/>
          <ci>v3</ci>
          <apply>
            <plus/>
            <ci>v4</ci>
            <ci>J</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
  <component cmeta:id="S4_ex" name="S4_ex">
    
