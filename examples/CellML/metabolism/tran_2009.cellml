          <eq/>
          <ci>dforce</ci>
          <apply>
             <plus/>
             <apply>
                <times/>
                <ci>kxb</ci>
                <ci>dSOVFThick</ci>
                <apply>
                   <plus/>
                   <apply>
                      <times/>
