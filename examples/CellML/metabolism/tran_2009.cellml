                <apply>
                   <plus/>
                   <apply>
                      <times/>
                      <ci>dxXBpostr</ci>
                      <ci>XBpostr</ci>
                   </apply>
                   <apply>
                      <times/>
                      <ci>xXBpostr</ci>
                      <ci>dXBpostr</ci>
