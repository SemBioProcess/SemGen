                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                            <ci>FrSBXB</ci>
                         </apply>
                         <ci>TRPNCaL</ci>
                      </apply>
                   </apply>
                </apply>
                <apply>
                   <times/>
                   <ci>SOVFThin</ci>
                   <apply>
                      <minus/>
                      <apply>
                         <plus/>
                         <apply>
                            <times/>
                            <ci>dFrSBXB</ci>
                            <ci>TRPNCaH</ci>
                         </apply>
                         <apply>
                            <times/>
                            <ci>FrSBXB</ci>
                            <ci>dTRPNCaH</ci>
                         </apply>
                         <apply>
                            <times/>
                            <apply>
                               <minus/>
                               <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                               <ci>FrSBXB</ci>
                            </apply>
                            <ci>dTRPNCaL</ci>
                         </apply>
                      </apply>
                      <apply>
                         <times/>
                         <ci>dFrSBXB</ci>
                         <ci>TRPNCaL</ci>
                      </apply>
                   </apply>
                </apply>
             </apply>
          </apply>
       </apply>
