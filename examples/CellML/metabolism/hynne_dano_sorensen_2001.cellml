                      <ci> K15_INAD </ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> DHAP </ci>
              <apply>
                <plus/>
                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <times/>
                  <apply>
