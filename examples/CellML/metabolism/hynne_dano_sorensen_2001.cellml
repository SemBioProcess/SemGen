        <ci> lpGlyc </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> V_15m </ci>
            <ci> DHAP </ci>
          </apply>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> K15_DHAP </ci>
              <apply>
                <plus/>
                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                <apply>
