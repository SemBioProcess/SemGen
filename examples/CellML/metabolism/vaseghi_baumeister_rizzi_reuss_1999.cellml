                  </apply>
                  <apply>
                    <times/>
                    <cn cellml:units="per_second2">0.05</cn>
                    <apply>
                      <power/>
                      <ci>time</ci>
                      <cn cellml:units="dimensionless">2</cn>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <plus/>
              <cn cellml:units="dimensionless">13.42</cn>
              <apply>
                <times/>
                <cn cellml:units="per_second">0.1</cn>
                <ci>time</ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math> 
