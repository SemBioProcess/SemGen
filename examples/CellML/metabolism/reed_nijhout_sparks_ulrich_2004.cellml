         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_GNMT">
      <variable name="V_GNMT" units="flux" public_interface="out"/>
      <variable name="AdoMet" units="micromolar" public_interface="in"/>
      <variable name="AdoHcy" units="micromolar" public_interface="in"/>
      <variable name="V_GNMTmax" units="flux" initial_value="10600"/>
      <variable name="Km_GNMT" units="micromolar" initial_value="4500"/>
      <variable name="Ki_GNMT" units="micromolar" initial_value="20"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_GNMT</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <apply>
                     <divide/>
                     <ci>V_GNMTmax</ci>
                     <apply>
                        <plus/>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                           <power/>
                           <apply>
                              <divide/>
                              <ci>Km_GNMT</ci>
                              <ci>AdoMet</ci>
                           </apply>
                           <cn cellml:units="dimensionless">2.3</cn>
                        </apply>
                     </apply>
                  </apply>
                  <cn cellml:units="dimensionless">1</cn>
               </apply>
               <apply>
                  <plus/>
                  <cn cellml:units="dimensionless">1</cn>
                  <apply>
                     <divide/>
                     <ci>AdoHcy</ci>
                     <ci>Ki_GNMT</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_METH">
      <variable name="V_METH" units="flux" public_interface="out"/>
      <variable name="AdoMet" units="micromolar" public_interface="in"/>
      <variable name="AdoHcy" units="micromolar" public_interface="in"/>
      <variable name="V_METHmax" units="flux" initial_value="4521"/>
      <variable name="Km1_METH" units="micromolar"/>
      <variable name="Km2_METH_A" units="dimensionless" initial_value="10"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_METH</ci>
            <apply>
               <divide/>
               <ci>V_METHmax</ci>
               <apply>
                  <plus/>
                  <cn cellml:units="dimensionless">1</cn>
                  <apply>
                     <divide/>
                     <ci>Km1_METH</ci>
                     <ci>AdoMet</ci>
                  </apply>
                  <ci>Km2_METH_A</ci>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
                        <ci>Km2_METH_A</ci>
                        <ci>Km1_METH</ci>
                     </apply>
                     <ci>AdoMet</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>Km1_METH</ci>
            <apply>
               <times/>
               <cn cellml:units="micromolar">1</cn>
               <apply>
                  <plus/>
                  <cn cellml:units="dimensionless">1</cn>
                  <apply>
                     <divide/>
                     <ci>AdoHcy</ci>
                     <cn cellml:units="micromolar">4</cn>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_MS">
      <variable name="V_MS" units="flux" public_interface="out"/>
      <variable name="five_mTHF" units="micromolar" initial_value="5.2"/>
      <variable name="Hcy" units="micromolar" public_interface="in"/>
      <variable name="V_MSmax" units="flux" initial_value="500"/>
      <variable name="Kd_MS" units="micromolar" initial_value="1"/>
      <variable name="Km_Hcy_MS" units="micromolar" initial_value="0.1"/>
      <variable name="Km_five_mTHF_MS" units="micromolar" initial_value="25"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_MS</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <ci>V_MSmax</ci>
                  <ci>five_mTHF</ci>
                  <ci>Hcy</ci>
               </apply>
               <apply>
                  <plus/>
                  <apply>
                     <times/>
                     <ci>Kd_MS</ci>
                     <ci>Km_Hcy_MS</ci>
                  </apply>
                  <apply>
                     <times/>
                     <ci>Km_Hcy_MS</ci>
                     <ci>five_mTHF</ci>
                  </apply>
                  <apply>
                     <times/>
                     <ci>Km_five_mTHF_MS</ci>
                     <ci>Hcy</ci>
                  </apply>
                  <apply>
                     <times/>
                     <ci>five_mTHF</ci>
                     <ci>Hcy</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_AH">
      <variable name="V_AH" units="flux" public_interface="out"/>
      <variable name="AdoHcy" units="micromolar" public_interface="in"/>
      <variable name="Hcy" units="micromolar" public_interface="in"/>
      <variable name="alpha1" units="first_order_rate_constant" initial_value="100"/>
      <variable name="alpha2" units="dimensionless" initial_value="10"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_AH</ci>
            <apply>
               <times/>
               <ci>alpha1</ci>
               <apply>
                  <minus/>
                  <ci>AdoHcy</ci>
                  <apply>
                     <times/>
                     <ci>alpha2</ci>
                     <ci>Hcy</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_CBS">
      <variable name="V_CBS" units="flux" public_interface="out"/>
      <variable name="AdoHcy" units="micromolar" public_interface="in"/>
      <variable name="Hcy" units="micromolar" public_interface="in"/>
      <variable name="AdoMet" units="micromolar" public_interface="in"/>
      <variable name="beta1" units="second_order_rate_constant" initial_value="1.7"/>
      <variable name="beta2" units="first_order_rate_constant" initial_value="30"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_CBS</ci>
            <apply>
               <times/>
               <apply>
                  <minus/>
                  <apply>
                     <times/>
                     <ci>beta1</ci>
                     <apply>
                        <plus/>
                        <ci>AdoMet</ci>
                        <ci>AdoHcy</ci>
                     </apply>
                  </apply>
                  <ci>beta2</ci>
               </apply>
               <ci>Hcy</ci>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="V_BHMT">
      <variable name="V_BHMT" units="flux" public_interface="out"/>
      <variable name="AdoMet" units="micromolar" public_interface="in"/>
      <variable name="AdoHcy" units="micromolar" public_interface="in"/>
      <variable name="Hcy" units="micromolar" public_interface="in"/>
      <variable name="V_BHMTmax" units="flux" initial_value="2500"/>
      <variable name="Km_BHMT" units="micromolar" initial_value="12"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_BHMT</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <apply>
                     <minus/>
                     <cn cellml:units="dimensionless">0.7</cn>
                     <apply>
                        <times/>
                        <cn cellml:units="per_micromolar">0.025</cn>
                        <apply>
                           <minus/>
                           <apply>
                              <plus/>
                              <ci>AdoMet</ci>
                              <ci>AdoHcy</ci>
                           </apply>
                           <cn cellml:units="micromolar">150</cn>
                        </apply>
                     </apply>
                  </apply>
                  <ci>V_BHMTmax</ci>
                  <ci>Hcy</ci>
               </apply>
               <apply>
                  <plus/>
                  <ci>Km_BHMT</ci>
                  <ci>Hcy</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
