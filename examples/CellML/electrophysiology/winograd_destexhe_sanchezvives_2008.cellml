                        </apply>
                     </apply>
                     <cn cellml:units="dimensionless">1e-6</cn>
                  </apply>
               </piece>
               <otherwise>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
                        <apply>
                           <minus/>
                           <cn cellml:units="per_second_per_millivolt">0.28</cn>
                        </apply>
                        <apply>
                           <minus/>
