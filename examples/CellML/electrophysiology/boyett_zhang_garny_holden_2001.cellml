                  </apply>
                  <ci>F</ci>
               </apply>
               <apply>
                  <ln/>
                  <apply>
                     <divide/>
                     <apply>
                        <plus/>
                        <ci>K_o</ci>
                        <apply>
                           <times/>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0.12</cn>
                           <ci>Na_o</ci>
                        </apply>
                     </apply>
                     <apply>
                        <plus/>
                        <ci>K_i</ci>
                        <apply>
                           <times/>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">0.12</cn>
                           <ci>Na_i</ci>
                        </apply>
                     </apply>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   
