    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="d_s_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> d_s </ci>
