        <ci> tau_d_s </ci>
        <apply>
          <divide/>
          <cn cellml:units="dimensionless"> 1.0 </cn>
          <apply>
            <plus/>
            <ci> alpha_d_s </ci>
            <ci> beta_d_s </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="d_s_infinity_calculation">
        <eq/>
        <ci> d_s_infinity </ci>
        <apply>
          <divide/>
          <ci> alpha_d_s </ci>
          <apply>
            <plus/>
            <ci> alpha_d_s </ci>
            <ci> beta_d_s </ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
