      <apply id="alpha_d_s_calculation">
        <eq/>
        <ci> alpha_d_s </ci>
        <apply>
          <divide/>
          <cn cellml:units="per_second"> 1000.0 </cn>
          <apply>
            <plus/>
            <apply>
              <times/>
