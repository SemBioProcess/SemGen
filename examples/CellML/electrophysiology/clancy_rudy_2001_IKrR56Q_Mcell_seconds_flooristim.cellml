                                    <apply>
                                       <minus/>
                                       <ci>time</ci>
                                       <ci>stim_start</ci>
                                    </apply>
                                    <ci>stim_period</ci>
                                 </apply>
                              </apply>
                              <ci>stim_period</ci>
                           </apply>
                        </apply>
                        <ci>stim_duration</ci>
                     </apply>
                  </apply>
               </piece>
               <otherwise>
                  <cn cellml:units="microA_per_microF">0</cn>
               </otherwise>
            </piecewise>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
