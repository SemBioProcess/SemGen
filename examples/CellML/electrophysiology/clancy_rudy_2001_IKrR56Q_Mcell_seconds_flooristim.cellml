         <apply>
            <eq/>
            <ci>I_st</ci>
            <piecewise>
               <piece>
                  <ci>stim_amplitude</ci>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <ci>time</ci>
                        <ci>stim_start</ci>
                     </apply>
                     <apply>
                        <leq/>
                        <ci>time</ci>
                        <ci>stim_end</ci>
                     </apply>
                     <apply>
                        <leq/>
                        <apply>
                           <minus/>
                           <apply>
                              <minus/>
                              <ci>time</ci>
                              <ci>stim_start</ci>
                           </apply>
                           <apply>
                              <times/>
                              <apply>
