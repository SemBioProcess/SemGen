      <apply id="beta_K1_calculation">
        <eq/>
        <ci> beta_K1 </ci>
        <apply>
          <divide/>
          <apply>
            <plus/>
            <apply>
              <times/>
              <cn cellml:units="per_millisecond"> 0.49124 </cn>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <cn cellml:units="dimensionless"> 0.08032 </cn>
                  <apply>
                    <minus/>
                    <apply>
                      <plus/>
                      <ci> V </ci>
                      <cn cellml:units="millivolt"> 5.476 </cn>
                    </apply>
                    <ci> E_K1 </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <exp/>
              <apply>
                <times/>
                <cn cellml:units="dimensionless"> 0.06175 </cn>
                <apply>
                  <minus/>
                  <ci> V </ci>
                  <apply>
                    <plus/>
                    <ci> E_K1 </ci>
                    <cn cellml:units="millivolt"> 594.31 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <times/>
                <cn cellml:units="per_millivolt"> -0.5143 </cn>
                <apply>
                  <minus/>
                  <ci> V </ci>
                  <apply>
                    <plus/>
                    <ci> E_K1 </ci>
                    <cn cellml:units="millivolt"> 4.753 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
          
