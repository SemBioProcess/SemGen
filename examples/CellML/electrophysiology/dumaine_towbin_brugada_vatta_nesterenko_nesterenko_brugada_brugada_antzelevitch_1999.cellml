        <ci> alpha_K1 </ci>
        <apply>
          <divide/>
          <cn cellml:units="per_millisecond"> 1.02 </cn>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
