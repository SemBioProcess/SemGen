   <component name="ICC_Membrane">
      <variable initial_value="0.025" name="Cm" units="capacitance_units"/>

      <variable initial_value="1e-12" name="Vol" private_interface="out" public_interface="out" units="volume_units"/>
      <variable initial_value="0.7" name="P_cyto" private_interface="out" public_interface="out" units="dimensionless"/>
      <variable name="V_cyto" private_interface="out" units="volume_units"/>
      <variable initial_value="0.01" name="fc" private_interface="out" units="dimensionless"/>
      <variable cmeta:id="Vm" initial_value="-67" name="Vm" private_interface="out" units="voltage_units"/>
      <variable name="time" private_interface="out" public_interface="in" units="time_units"/>
      <variable initial_value="0.00000993087" name="Ca_i" private_interface="out" public_interface="out" units="millimolar"/>
      <variable initial_value="30" name="Na_i" private_interface="out" public_interface="out" units="millimolar"/>
      <variable initial_value="120" name="K_i" private_interface="out" units="millimolar"/>

      <variable initial_value="88" name="Cl_i" private_interface="out" units="millimolar"/>
      <variable name="Ca_o" private_interface="out" public_interface="in" units="millimolar"/>
      <variable name="Na_o" private_interface="out" public_interface="in" units="millimolar"/>
      <variable name="K_o" private_interface="out" public_interface="in" units="millimolar"/>
      <variable name="Cl_o" private_interface="out" public_interface="in" units="millimolar"/>
      <variable name="T_correction_Ca" private_interface="out" public_interface="in" units="dimensionless"/>
      <variable name="T_correction_Na" private_interface="out" public_interface="in" units="dimensionless"/>
      <variable name="T_correction_K" private_interface="out" public_interface="in" units="dimensionless"/>
      <variable name="T_correction_BK" private_interface="out" public_interface="in" units="conductance_units"/>

      <variable name="RToF" private_interface="out" public_interface="in" units="voltage_units"/>
      <variable name="FoRT" private_interface="out" public_interface="in" units="Inverse_Voltage_units"/>
      <variable name="F" private_interface="out" public_interface="in" units="F_units"/>
      <variable name="J_leak" private_interface="in" units="millimolar_per_second"/>
      <variable name="I_Na" private_interface="in" units="current_units"/>
      <variable name="I_Ltype" private_interface="in" units="current_units"/>
      <variable name="I_VDDR" private_interface="in" units="current_units"/>
      <variable name="I_kv11" private_interface="in" units="current_units"/>
      <variable name="I_BK" private_interface="in" units="current_units"/>

      <variable name="I_ERG" private_interface="in" units="current_units"/>
      <variable name="I_CaCl" private_interface="in" units="current_units"/>
      <variable name="I_NSCC" private_interface="in" units="current_units"/>
      <variable name="I_bk" private_interface="in" units="current_units"/>
      <variable name="J_PMCA" private_interface="in" units="millimolar_per_second"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_cyto</ci>

            <apply>
               <times/>
               <ci>Vol</ci>
               <ci>P_cyto</ci>
            </apply>
         </apply>
         <apply>
            <eq/>

            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Vm</ci>
            </apply>
            <apply>

               <times/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <apply>
                        <minus/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>

                     </apply>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                  </apply>
                  <ci>Cm</ci>
               </apply>
               <apply>
                  <plus/>
                  <ci>I_Na</ci>

                  <ci>I_Ltype</ci>
                  <ci>I_VDDR</ci>
                  <ci>I_kv11</ci>
                  <ci>I_ERG</ci>
                  <ci>I_BK</ci>
                  <ci>I_CaCl</ci>

                  <ci>I_NSCC</ci>
                  <ci>I_bk</ci>
                  <apply>
                     <times/>
                     <ci>J_PMCA</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millimole_to_nanomole">1000000</cn>

                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="microcoulomb_to_picocoulomb">1000000</cn>
                     <ci>F</ci>
                     <ci>V_cyto</ci>
                  </apply>
               </apply>
            </apply>
         </apply>

         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_i</ci>

            </apply>
            <apply>
               <times/>
               <ci>fc</ci>
               <apply>
                  <plus/>
                  <apply>
                     <divide/>

                     <apply>
                        <plus/>
                        <apply>
                           <times/>
                           <apply>
                              <minus/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                           </apply>

                           <ci>I_Ltype</ci>
                        </apply>
                        <apply>
                           <times/>
                           <apply>
                              <minus/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                           </apply>

                           <ci>I_VDDR</ci>
                        </apply>
                     </apply>
                     <apply>
                        <times/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="millimole_to_nanomole">1000000</cn>

                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="microcoulomb_to_picocoulomb">1000000</cn>
                        <ci>F</ci>
                        <ci>V_cyto</ci>
                     </apply>
                  </apply>
                  <ci>J_leak</ci>
                  <apply>

                     <times/>
                     <apply>
                        <minus/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                     </apply>
                     <ci>J_PMCA</ci>
                  </apply>
               </apply>

            </apply>
         </apply>
      </math>
   </component>
   <component name="d_Ltype">
      <variable name="Vm" public_interface="in" units="voltage_units"/>
      <variable name="d_inf_Ltype" units="dimensionless"/>
      <variable name="tau_d_Ltype" units="time_units"/>
      <variable initial_value="0" name="d_Ltype" public_interface="out" units="dimensionless"/>

      <variable name="time" public_interface="in" units="time_units"/>
      <variable name="T_correction_Ca" public_interface="in" units="dimensionless"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>d_inf_Ltype</ci>
            <apply>
               <divide/>

               <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
               <apply>
                  <plus/>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
                  <apply>
                     <exp/>
                     <apply>
                        <divide/>

                        <apply>
                           <plus/>
                           <ci>Vm</ci>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="voltage_units">17</cn>
                        </apply>
                        <apply>
                           <minus/>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="voltage_units">4.3</cn>

                        </apply>
                     </apply>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>tau_d_Ltype</ci>

            <apply>
               <times/>
               <ci>T_correction_Ca</ci>
               <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="time_units">0.001</cn>
            </apply>
         </apply>
         <apply>
            <eq/>

            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>d_Ltype</ci>
            </apply>
            <apply>

               <divide/>
               <apply>
                  <minus/>
                  <ci>d_inf_Ltype</ci>
                  <ci>d_Ltype</ci>
               </apply>
               <ci>tau_d_Ltype</ci>

            </apply>
         </apply>
      </math>
   </component>
