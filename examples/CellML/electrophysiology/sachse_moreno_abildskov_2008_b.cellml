   <component cmeta:id="eqn14" name="I_b">
      <variable units="nanoampere" public_interface="out" name="I_b"/>
      <variable units="microsiemens" name="Gb" initial_value="6.9e-6"/>
      <variable units="millivolt" name="Eb" initial_value="0"/>
      <variable units="second" public_interface="in" name="time"/>
      <variable units="millivolt" public_interface="in" name="Vm"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_b</ci>
            <apply>
               <times/>
               <ci>Gb</ci>
               <apply>
                  <minus/>
                  <ci>Vm</ci>
                  <ci>Eb</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component name="I_stim">
      <variable units="nanoampere" public_interface="out" name="I_stim"/>
      <variable units="second" name="stim_start" initial_value="0.1"/>
      <variable units="second" name="stim_end" initial_value="10"/>
      <variable units="second" name="stim_period" initial_value="1"/>
      <variable units="second" name="stim_duration" initial_value="0.001"/>
      <variable units="nanoampere" name="stim_amplitude" initial_value="0.1"/>
      <variable units="second" public_interface="in" name="time"/>
      <variable units="millivolt" public_interface="in" name="Vm"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_stim</ci>
            <piecewise>
               <piece>
                  <ci>stim_amplitude</ci>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <ci>time</ci>
                        <ci>stim_start</ci>
                     </apply>
                  <apply>
                     <and/>
                     <apply>
                        <leq/>
                        <ci>time</ci>
                        <ci>stim_end</ci>
                     </apply>
                     <apply>
                        <leq/>
                        <apply>
                           <minus/>
                           <apply>
                              <minus/>
                              <ci>time</ci>
                              <ci>stim_start</ci>
                           </apply>
                           <apply>
                              <times/>
                              <apply>
                                 <floor/>
                                 <apply>
                                    <divide/>
                                    <apply>
                                       <minus/>
                                       <ci>time</ci>
                                       <ci>stim_start</ci>
                                    </apply>
                                    <ci>stim_period</ci>
                                 </apply>
                              </apply>
                              <ci>stim_period</ci>
                           </apply>
                        </apply>
                        <ci>stim_duration</ci>
                     </apply>
                  </apply>
                  </apply>
               </piece>
               <otherwise>
                  <cn cellml:units="nanoampere">0</cn>
               </otherwise>
            </piecewise>
         </apply>
      </math>
   </component>
   <component name="model_parameters">
      <variable units="millijoule_per_kelvin_mole" public_interface="out" name="R" initial_value="8310"/>
      <variable units="kelvin" public_interface="out" name="T" initial_value="295"/>
      <variable units="coulomb_per_mole" public_interface="out" name="F" initial_value="9.65e4"/>
      <variable units="millimolar" public_interface="out" name="Ki" initial_value="140"/>
      <variable units="millimolar" public_interface="out" name="Ko" initial_value="5"/>
   </component>
   <group>
