   <component cmeta:id="eqn2" name="I_Kir">
      <variable units="nanoampere" public_interface="out" name="I_Kir"/>
      <variable units="microsiemens" name="GKir" initial_value="1e-3"/>
      <variable units="dimensionless" name="aKir" initial_value="0.94"/>
      <variable units="dimensionless" name="bKir" initial_value="1.26"/>
      <variable units="millivolt" name="EK"/>
      <variable units="dimensionless" name="OKir"/>
      <variable units="second" public_interface="in" name="time"/>
      <variable units="millivolt" public_interface="in" name="Vm"/>
      <variable units="millijoule_per_kelvin_mole" public_interface="in" name="R"/>
      <variable units="kelvin" public_interface="in" name="T"/>
      <variable units="coulomb_per_mole" public_interface="in" name="F"/>
      <variable units="millimolar" public_interface="in" name="Ko"/>
      <variable units="millimolar" public_interface="in" name="Ki"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_Kir</ci>
            <apply>
               <times/>
               <ci>GKir</ci>
               <ci>OKir</ci>
               <apply>
                  <root/>
                  <apply>
                     <times/>
                     <ci>Ko</ci>
                     <cn cellml:units="per_millimolar">1e-3</cn>
                  </apply>
               </apply>
               <apply>
                  <minus/>
                  <ci>Vm</ci>
                  <ci>EK</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>OKir</ci>
            <apply>
               <divide/>
               <cn cellml:units="dimensionless">1</cn>
               <apply>
                  <plus/>
                  <ci>aKir</ci>
                  <apply>
                     <exp/>
                     <apply>
                        <divide/>
                        <apply>
                           <times/>
                           <ci>bKir</ci>
                           <apply>
                              <minus/>
                              <ci>Vm</ci>
                              <ci>EK</ci>
                           </apply>
                           <ci>F</ci>
                        </apply>
                        <apply>
                           <times/>
                           <ci>R</ci>
                           <ci>T</ci>
                        </apply>
                     </apply>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>EK</ci>
            <apply>
               <times/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <ci>R</ci>
                     <ci>T</ci>
                  </apply>
                  <ci>F</ci>
               </apply>
               <apply>
                  <ln/>
                  <apply>
                     <divide/>
                     <ci>Ko</ci>
                     <ci>Ki</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
