      <variable units="second" public_interface="in" name="time"/>
      <variable units="millivolt" name="VmReal" initial_value="-58.00"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>Vm</ci>
            <piecewise>
               <piece>
                  <cn cellml:units="millivolt">1e-7</cn>
                  <apply>
                     <lt/>
                     <apply>
                        <abs/>
                        <ci>VmReal</ci>
                     </apply>
                     <cn cellml:units="millivolt">1e-7</cn>
                  </apply>
               </piece>
               <otherwise>
                  <ci>VmReal</ci>
               </otherwise>
            </piecewise>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>VmReal</ci>
            </apply>
            <apply>
               <divide/>
               <apply>
                  <minus/>
                  <apply>
                     <minus/>
                     <apply>
                        <plus/>
                        <ci>I_Kir</ci>
                        <ci>I_Shkr</ci>
                        <ci>I_b</ci>
                     </apply>
                     <ci>I_stim</ci>
                  </apply>
               </apply>
               <ci>Cm</ci>
            </apply>
         </apply>
      </math>
