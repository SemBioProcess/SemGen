               <apply>
                  <times/>
                  <ci>vmp</ci>
                  <apply>
                     <power/>
                     <ci>Ca_i</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                  </apply>
               </apply>
               <apply>
                  <plus/>
                  <apply>
                     <power/>
                     <ci>Ca_i</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                  </apply>
                  <apply>
                     <power/>
                     <ci>kmp</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>J_mem_tot</ci>
            <apply>
               <times/>
               <apply>
                  <minus/>
                  <ci>f</ci>
               </apply>
               <apply>
                  <plus/>
                  <apply>
                     <times/>
                     <ci>gamma</ci>
                     <ci>i_Ca</ci>
                  </apply>
                  <ci>Jmp</ci>
               </apply>
            </apply>
         </apply>
      </math>
