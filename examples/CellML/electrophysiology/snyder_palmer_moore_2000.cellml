      <unit units="second" exponent="-1"/>
   </units>
   <units xmlns="http://www.cellml.org/cellml/1.0#" name="second_order_rate_constant">
      <unit units="molar" exponent="-1"/>
      <unit units="second" exponent="-1"/>
   </units>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="environment">
      <variable name="time" units="second" public_interface="out"/>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="Ca_influx_across_the_SR">
      <variable name="Rate_Ca_influx_across_the_SR" units="flux" public_interface="out"/>
      <variable name="Ca_e" units="molar" public_interface="in"/>
      <variable name="Ca_f" units="molar" public_interface="in"/>
      <variable name="k1" units="first_order_rate_constant" initial_value="0.2"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>Rate_Ca_influx_across_the_SR</ci>
            <apply>
               <times/>
               <ci>k1</ci>
               <apply>
                  <minus/>
                  <ci>Ca_e</ci>
                  <ci>Ca_f</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="Ca_movement_through_the_SRRC">
      <variable name="Ca_2_S1" units="molar" public_interface="in"/>
      <variable name="Ca_S2" units="molar" public_interface="in"/>
      <variable name="S1" units="molar" public_interface="in"/>
      <variable name="S2" units="molar" public_interface="in"/>
      <variable name="Ca_f" units="molar" public_interface="in"/>
      <variable name="Ca_s" units="molar" public_interface="in"/>
      <variable name="dCa2_S1_dt" units="flux" public_interface="out"/>
      <variable name="dCa_S2_dt" units="flux" public_interface="out"/>
      <variable name="k_on1" units="second_order_rate_constant" initial_value="2000000000"/>
      <variable name="k_off1" units="first_order_rate_constant" initial_value="1400"/>
      <variable name="k_on2" units="second_order_rate_constant" initial_value="13000000"/>
      <variable name="k_off2" units="first_order_rate_constant" initial_value="3.9"/>
      <variable name="k_s" units="first_order_rate_constant" initial_value="9"/>
      <variable name="cas1" units="dimensionless" initial_value="0"/>
      <variable name="cas2" units="dimensionless" initial_value="0"/>
      <variable name="dcas1_dt" units="first_order_rate_constant" public_interface="out"/>
      <variable name="dcas2_dt" units="first_order_rate_constant" public_interface="out"/>
      <variable name="r_o" units="dimensionless"/>
      <variable name="Rate_Ca_movement_through_the_SRRC" units="flux" public_interface="out"/>
      <variable name="time" units="second" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>dCa2_S1_dt</ci>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>k_on1</ci>
                  <ci>Ca_f</ci>
                  <ci>S1</ci>
               </apply>
               <apply>
                  <times/>
                  <apply>
                     <divide/>
                     <apply>
                        <power/>
                        <ci>k_off1</ci>
                        <cn cellml:units="dimensionless">2</cn>
                     </apply>
                     <apply>
                        <times/>
                        <ci>k_on1</ci>
                        <ci>Ca_f</ci>
                     </apply>
                  </apply>
                  <ci>Ca_2_S1</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>dCa_S2_dt</ci>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>k_on2</ci>
                  <ci>Ca_f</ci>
                  <ci>S2</ci>
               </apply>
               <apply>
                  <times/>
                  <ci>k_off2</ci>
                  <ci>Ca_S2</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>r_o</ci>
            <apply>
               <times/>
               <ci>cas1</ci>
               <apply>
                  <minus/>
                  <cn cellml:units="dimensionless">1</cn>
                  <ci>cas2</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>cas1</ci>
            </apply>
            <ci>dcas1_dt</ci>
         </apply>
         <apply>
            <eq/>
            <ci>dcas1_dt</ci>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>k_on1</ci>
                  <ci>Ca_f</ci>
                  <apply>
                     <minus/>
                     <cn cellml:units="dimensionless">1</cn>
                     <ci>cas1</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <apply>
                     <divide/>
                     <apply>
                        <power/>
                        <ci>k_off1</ci>
                        <cn cellml:units="dimensionless">2</cn>
                     </apply>
                     <apply>
                        <times/>
                        <ci>k_on1</ci>
                        <ci>Ca_f</ci>
                     </apply>
                  </apply>
                  <ci>cas1</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>cas2</ci>
            </apply>
            <ci>dcas2_dt</ci>
         </apply>
         <apply>
            <eq/>
            <ci>dcas2_dt</ci>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>k_on2</ci>
                  <ci>Ca_f</ci>
                  <apply>
                     <minus/>
                     <cn cellml:units="dimensionless">1</cn>
                     <ci>cas2</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>k_off2</ci>
                  <ci>cas2</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>Rate_Ca_movement_through_the_SRRC</ci>
            <apply>
               <times/>
               <ci>k_s</ci>
               <ci>r_o</ci>
               <apply>
                  <minus/>
                  <ci>Ca_s</ci>
                  <ci>Ca_f</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="Ca_efflux_across_the_SR_by_NaCa_exchange">
