            <cn cellml:units="millisecond"> 12.0 </cn>
            <apply>
              <gt/>
              <ci> V </ci>
              <cn cellml:units="millivolt"> 0.0 </cn>
            </apply>
          </piece>
          <otherwise>
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn cellml:units="per_millivolt_millisecond"> -0.875 </cn>
                <ci> V </ci>
              </apply>
              <cn cellml:units="millisecond"> 12.0 </cn>
            </apply>
          </otherwise>
        </piecewise>
      </apply>
    </math>
  </component>  
  
