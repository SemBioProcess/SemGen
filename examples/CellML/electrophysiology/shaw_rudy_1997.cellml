        <ci> g_K_ATP </ci>
        <apply>
          <times/>
          <ci> G_K_ATP </ci>
          <ci> P_ATP </ci>
          <apply>
            <power/>
            <apply>
              <divide/>
              <ci> Ko </ci>
              <ci> Ko_normal </ci>
            </apply>
            <ci> n </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="E_K_calculation">
        <eq/>
        <ci> E_K </ci>
