                <ci> gamma_Ki </ci>
                <ci> Ki </ci>
                <apply>
                  <exp/>
                  <apply>
                    <divide/>
                    <apply>
                      <times/>
                      <cn cellml:units="dimensionless"> 1.0 </cn>
