                  <cn cellml:units="millivolt"> 6.24 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="tau_d_calculation">
        <eq/>
        <ci> tau_d </ci>
        <apply>
          <times/>
          <ci> d_infinity </ci>
          <apply>
            <divide/>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <apply>
                <exp/>
                <apply>
                  <minus/>
                  <apply>
                    <divide/>
                    <apply>
