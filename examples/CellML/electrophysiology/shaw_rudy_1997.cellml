          </apply>
        </apply>
      </apply>
      
      <apply id="P_Ca_calculation">
        <eq/>
        <ci> P_Ca </ci>
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <log/>
              <ci> Cai </ci>
            </apply>
          </apply>
          <cn cellml:units="dimensionless"> 3.0 </cn>
        </apply>
      </apply>

      <apply id="i_Ks_calculation">
        <eq/>
        <ci> i_Ks </ci>
        <apply>
          <times/>
          <ci> g_Ks </ci>
          <apply>
            <power/>
            <ci> Xs </ci>
            <cn cellml:units="dimensionless"> 2.0 </cn>
          </apply>
          <apply>
            <minus/>
            <ci> V </ci>
            <ci> E_Ks </ci>
          </apply>
        </apply>
      </apply>
    </math>
