        </apply>
      </apply>
      
      <apply id="E_Ca_calculation">
        <eq/>
        <ci> E_Ca </ci>
        <apply>
          <times/>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> R </ci>
              <ci> T </ci>
            </apply>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> 2.0 </cn>
              <ci> F </ci>
            </apply>
          </apply>
          <apply>
            <ln/>                
