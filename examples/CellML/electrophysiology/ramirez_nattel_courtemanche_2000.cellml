    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="Na_i_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> Na_i </ci>
        </apply> 
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> -3.0 </cn>
              <ci> i_NaK </ci>
            </apply>
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn cellml:units="dimensionless"> 3.0 </cn>
                <ci> i_NaCa </ci>
              </apply>
              <ci> i_B_Na </ci>
              <ci> i_Na </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> V_i </ci>
            <ci> F </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="K_i_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> K_i </ci>
        </apply> 
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> 2.0 </cn>
              <ci> i_NaK </ci>
            </apply>
            <apply>
              <plus/>
              <ci> i_K1 </ci>
              <ci> i_to </ci>
              <ci> i_Kur_d </ci>
              <ci> i_Kr </ci>
              <ci> i_Ks </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> V_i </ci>
            <ci> F </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="Cl_i_diff_eq">
