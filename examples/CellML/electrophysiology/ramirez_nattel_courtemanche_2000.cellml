        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> Cl_i </ci>
        </apply> 
        <apply>
          <divide/>
          <ci> i_Cl_Ca </ci>
          <apply>
            <times/>
            <ci> V_i </ci>
            <ci> F </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="Ca_i_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> Ca_i </ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <plus/>
            <apply>
              <divide/>
              <apply>
                <minus/>
                <apply>
                  <times/>
                  <cn cellml:units="dimensionless"> 2.0 </cn>
                  <ci> i_NaCa </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> i_p_Ca </ci>
