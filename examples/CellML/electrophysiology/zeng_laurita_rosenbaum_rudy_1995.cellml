                  <apply>
                     <times/>
                     <cn cellml:units="dimensionless">2</cn>
                     <ci>V_myo</ci>
                     <ci>F</ci>
                  </apply>
               </apply>
               <ci>R_A_V</ci>
            </apply>
         </apply>
      </math>
