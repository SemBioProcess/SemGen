                     <ci>i_leak</ci>
                     <ci>i_tr</ci>
                  </apply>
                  <ci>i_up</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_foot</ci>
