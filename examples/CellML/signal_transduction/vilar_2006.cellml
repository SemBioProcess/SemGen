      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> RI </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <plus/> 
            <ci> p_RI </ci>			 
