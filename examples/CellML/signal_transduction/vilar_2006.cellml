              <ci> RI </ci>
			  <ci> RII </ci>
		    </apply>
			<apply>
              <times/> 
			  <apply>
                <plus/>
		        <ci> kcd </ci>
			    <ci> ki </ci>
		      </apply>
              <ci> RI </ci>
			</apply>
          </apply>
        </apply>  
