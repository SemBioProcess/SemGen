    <variable initial_value="0.3" name="kappa_PI3K" units="dimensionless"/>
    <variable initial_value="80.0" name="alpha_PI3K" units="dimensionless"/>
    
	<variable name="c2" public_interface="in" units="dimensionless"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <ci>e_PI3K</ci> 
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <plus/>	
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>	
			  <ci> kappa_PI3K </ci>		  
			  <apply>
                <times/>
                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
				<ci> alpha_PI3K </ci>
		        <ci> c2 </ci>
			  </apply>
			</apply>
            <apply>
              <root/>
              <apply>
                <power/>
				<apply>
                  <plus/>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
				  <ci> kappa_PI3K </ci>		  
			      <apply>
                    <times/>
                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
				    <ci> alpha_PI3K </ci>
		            <ci> c2 </ci>
			      </apply>
			    </apply>
				<cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
              </apply>
            </apply> 
		  </apply>
		  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
        </apply>
      </apply>    
    </math>
  </component>

